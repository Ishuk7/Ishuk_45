//day 31
//45 days of code

#include <iostream>
using namespace std;

int main() 
{
	int t;
	cout << "Enter the number of times it needed to be run : ";
	cin >> t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    
	    if(n%2==0||n==1)
	    {
	        cout<<"YES\n";
	    }
	    else
	    {
	        cout<<"NO\n";
	    }
	    cout << "______________" << endl;
	}
	return 0;
}
