//Day 5
//45 Days Of Code

#include <iostream>
using namespace std;

int main() 
{
    int t;
    cout << "Enter the number of test case to be run : ";
    cin >> t;
    cout << endl;
    
    while(t--)
    {
        int x;
        x=x>0;
        cout << "Enter your battery percentage : ";
        cin >> x;
        
        if (x>15)
        {
            if (x<101)
            {
                cout << "Yes" << endl;
            }
            else
            {
                cout << "Error" << endl;
            }    
        }
        else
        {
            cout << "No" << endl;
        }
        cout << "_______________________________" << endl;
    }
    
    return 0;
}
