#include <iostream>
#include <cmath>
using namespace std;

int main() 
{
    int t;
    cout << "Enter the number of test needed to be run : ";
    cin >> t;

    while (t--)
    {
        int x, y;
        string num;
        cout << "Enter the number of followers : ";
        cin >> x;
        
        cout << "Enter the number of following : ";
        cin >> y;
        
        if (x>10*y)
        {
            cout << "Yes" << endl;
        }
        else
        {
            cout << "No" << endl;
        }
        cout << "__________________________________" <<endl;
    }

    return 0;
}
