//day 36
//45 Days Of Code

#include <iostream>
using namespace std;

int main() 
{
	int t;
	cout << "Enter the number of times it needed to be run : ";
	cin >> t;
	
	while(t--)
	{
	    double n, m;
	    cin >> n >> m;
	    if(m<n-(n*0.1))
	    {
	        cout<<"dining"<<endl;
	    }
	    else if(m>n-n*0.1)
	    {
	        cout<<"online"<<endl;
	    }
	    else
	    {
	        cout<<"either"<<endl;
	    }
	}
	return 0;
}
