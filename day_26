#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() 
{
	int t;
	cout << "Enter the number times to be run : ";
	cin>>t;
	
	while(t--)
	{
	    int x, y;
	    cin >> x >> y;
	    cout << abs(x-y) << endl;
	    cout << "_________________" << endl;
	}

	return 0;
}
