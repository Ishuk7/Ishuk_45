// Day 16
// 45 Days Of Code

#include <iostream>
using namespace std;

int main() 
{
    int t;
    cout << "Enter the number of test cases to be run : ";
    cin >> t;
    
    while(t--)
    {
        int n;
        cin >> n;
        
        if(n<=15)
        {
            if(n<=10)
            {
                cout << "Lower double" << endl;
            }
            else
            {
                cout << "Lower single" << endl;
            }
            
        }
        if((n>15)&& (n<31))
        {
            if((n<=25) && (n>=16))
            {
                cout << "Upper double" << endl;
            }
            else
            {
                cout << "Upper single" << endl;
            }
        }
        cout << "_______________" << endl;

    }
  
    return 0;
}
