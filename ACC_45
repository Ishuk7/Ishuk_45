#include <iostream>
using namespace std;

int main()
{
   int t;
   cout << "Doctor prescripted water level list";
   cout << endl;

   cout<<"Enter Number";
   cin >> t;
   while(t--)
  {
     int x;
     cin >> x;

     if((x>2000) || (x=2000))
     cout << "Yes";

     else
     cout << "No";
  }

cout << endl;
return 0;
}
