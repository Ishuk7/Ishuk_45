//day 32
//45 days of code

#include <bits/stdc++.h>
using namespace std;

int main() 
{
    int t;
    cout << "Enter the number of times it needed to be run : ";
    cin >> t;
    
    while(t--)
    {
        int a1, a2, b1, b2, p, s;
        cin >> a1 >> a2 >> b1 >> b2;
        p=a2+b2;
        s=a1+b1;
        if(s<p)
        {
            cout<<"YES"<<endl;
        }
        else
        {
            cout<<"NO"<<endl;
        }
        cout << "______________" << endl;
    }
	return 0;
}
