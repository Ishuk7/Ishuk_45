//Day 45
//45 Days Of Code

#include <iostream>
using namespace std;

int main() 
{
	int t;
	cin >> t;

	while(t--)
{
	    int n;
	    cin >> n;
	    int a[n];
	    for(int i=1;i<=n;i++)
      {
	        cin >> a[i];
	    }
	    int k;
	    cin >> k;
	    int count = 0;
	    for(int i=1;i<=n;i++)
      {
	        if(a[i]<a[k])
          {
	            count++;
	        }
	    }
	    cout << count+1 << endl;
	}
	return 0;
}
