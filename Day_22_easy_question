//Day 22
//45 Days Of Code

#include <iostream>
using namespace std;

int main() 
{
    int t;
    cout << "Enter the number of test cases to be run : ";
    cin >> t;
    
    while(t--)
    {
        int a, b, c, x, exp1, exp2, exp3;
        cin >> a >> b >> c >> x;
        exp1=a+b;
        exp2=b+c;
        exp3=a+c;
        
        if((exp1>=x) || (exp2>=x) || (exp3>=x))
        {
            cout << "Yes" << endl;
        }
        else 
        {
            cout << "No" << endl;
        }
        cout << "_______" << endl;
    }

    return 0;
}
