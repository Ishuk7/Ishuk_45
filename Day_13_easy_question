//Day 13
//45 Days Of Code

#include <iostream>
using namespace std;

int main() 
{
    int t;
    cout << "Enter the number of test cases to be run : ";
    cin >> t;
    
    while(t--)
    {
        int a, b, x, y, power, result;
        cin >> a >> b >> x >> y;
        power=a*b;
        result=x*y;
        
        if(power<=result)
        {
            cout << "Yes" << endl;
        }
        else
        {
            cout << "No" << endl;
        }
        cout << "_______________" << endl;
    }

    return 0;
}
