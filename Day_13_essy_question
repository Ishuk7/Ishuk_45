//Day 13
// 45 Days Of Code

#include <iostream>
using namespace std;

int main() {
    int t;
    cout << "Enter the number of test cases to be run : ";
    cin >> t;

    while (t--) 
    {
        int x, y, move;
        cin >> x >> y;
        move = x/y;
        
        if (x%y!=0) 
        {
            move++;
            cout << move << endl;
        }
        else
        {
        cout << move << endl;
        }
        cout << "_________" << endl;
    }

    return 0;
}
