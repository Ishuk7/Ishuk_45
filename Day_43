//day 43
//45 days of code

#include <iostream>
using namespace std;

int main() 
{
	int t;
	cin >> t;
	
	while(t--)
	{
	    int a;
	    cin >> a;
	    
	    int i;
	    for( i=2;i<=a;i++)
	    {
	        if(a%i==0)
	        {
	            break;
	        }
	    }
	    (i==a)?cout<<"yes":cout<<"no";
	    cout<<endl;
	}
	return 0;
}
