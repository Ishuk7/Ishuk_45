// Day 12
// 45 Days of Code

#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int a,b,i=1,j=2;
	    int n=i,m=j;
	    cin>>a>>b;
	    while(1)
	    {
	    	if(i<=a)
	    	{
	    		n=n+2;
	    		i+=n;
				if(j<=b)
	    		{
	    			m=m+2;
	    			j+=m;
				}
				else
        {
					cout<<"Limak"<<endl;
					break;
				}
			}
			else
			{
				cout<<"Bob"<<endl;
				break;
			}
			cout << "________________" << endl;
		}
	}   
	return 0;
}
