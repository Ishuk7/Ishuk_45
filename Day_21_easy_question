//Day 21
//45 Days Of Code

#include <iostream>
using namespace std;

int main() {
    int t;
    cout << "Enter the number of test cases to be run : ";
    cin >> t; 
    
    while (t--) 
    {
        int x, result;
        cin >> x;
        
        result=100-(x/3);
        
        cout << result << endl; 
        cout << "__________" << endl;
    }
    
    return 0;
}
